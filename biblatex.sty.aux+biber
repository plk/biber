\protected\def\blx@checksum{%
  \@tempcnta\z@
  \ifx\blx@checksum@old\blx@checksum@new
  \else
    \advance\@tempcnta\@ne
    \blx@reruntrue
  \fi
  \ifx\blx@auxlist\@empty
  \else
    \advance\@tempcnta\tw@
    \blx@reruntrue
  \fi
  \csuse{blx@rerun}%
  \ifcase\@tempcnta
  \or
    \blx@warn@rerun
  \else
    \blx@warn@auxlist
  \fi}

\let\blx@checksum@old\@empty
\let\blx@checksum@new\@empty
\let\bib@aux@checksum\relax

