  \global\toggletrue{blx@unit}}%

\def\blx@setunit@ii{%
  \iftoggle{blx@lastins}
    {\blx@setunit@i}
    {\@gobble}}

\protected\def\blx@finentry{%
  \unspace\finentrypunct
  \blx@postpunct
  \blx@initunit}

\protected\def\blx@finentry@inset{%
